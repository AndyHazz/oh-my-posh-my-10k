{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "newline": true,
          "background": "#f0f0f0",
          "foreground": "#1e1e1e",
          "leading_diamond": "\n\n<#7c7c7c,transparent>\u256d\u2500</>\ue0b6",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#7c3aed",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ if .SSHSession }} {{ end }}{{ .UserName }}{{ if .SSHSession }}@{{ .HostName }}{{ end }} "
        },
        {
          "background": "#2563eb",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "home_icon": "~",
            "style": "agnoster_short",
            "max_depth": 3,
            "folder_separator_icon": " \ue0b1 ",
            "max_width": 40,
            "mapped_locations": {
              "~/.config": "\ue5fc",
              "~/Documents": "\uf718",
              "~/Downloads": "\uf74c",
              "~/Pictures": "\uf755",
              "~/Music": "\uf886",
              "~/Videos": "\uf880",
              "~/Projects": "\uf489",
              "~/workspace": "\uf489",
              "~/dev": "\uf489"
            }
          },
          "style": "powerline",
          "template": "  {{ .Path }}  ",
          "type": "path"
        },
        {
          "background": "#10b981",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f59e0b{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ef4444{{ end }}",
            "{{ if gt .Ahead 0 }}#06b6d4{{ end }}",
            "{{ if gt .Behind 0 }}#8b5cf6{{ end }}"
          ],
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": "  {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}  ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "hide",
      "filler": "{{ if .Overflow }} {{ else }}<#6b7280,transparent></>{{ end}}",
      "segments": [
        {
          "background": "#22c55e",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}  ",
          "type": "node"
        },
        {
          "background": "#06b6d4",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "go"
        },
        {
          "background": "#8b5cf6",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "julia"
        },
        {
          "background": "#eab308",
          "foreground": "#1e1e1e",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "python"
        },
        {
          "background": "#dc2626",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "ruby"
        },
        {
          "background": "#f59e0b",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf0e7 ",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#f97316{{end}}",
            "{{if contains \"jan\" .Profile}}#ec4899{{end}}"
          ],
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": "  {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }}  ",
          "type": "aws"
        },
        {
          "background": "#facc15",
          "foreground": "#1e1e1e",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": "  \uf0ad  ",
          "type": "root"
        },
        {
          "background": "#f59e0b",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": "  {{ .FormattedMs }}  ",
          "type": "executiontime",
          "properties": {
            "threshold": 500,
            "style": "roundrock"
          }
        },
        {
          "background": "#334155",
          "background_templates": [
            "{{ if gt .Code 0 }}#ef4444{{ end }}"
          ],
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }} {{ reason .Code }}{{ else }} {{ end }}  ",
          "type": "status"
        },
        {
          "type": "battery",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "foreground": "#ffffff",
          "background": "#eab308",
          "background_templates": [
            "{{if lt (float64 .Percentage) 26}}#dc2626{{end}}",
            "{{if lt (float64 .Percentage) 76}}#06b6d4{{end}}",
            "{{if gt (float64 .Percentage) 75}}#10b981{{end}}"
          ],
          "template": "  {{ if not .Error }}{{ .Percentage }}% {{ end }}{{ .Icon}}{{ .Error }}  ",
          "properties": {
            "discharging_icon": "󰁿",
            "charging_icon": "󰂄",
            "charged_icon": "󱈑"
          }
        },
        {
          "background": "#f0f0f0",
          "foreground": "#1e1e1e",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b4<#7c7c7c,transparent>\u2500\u256e</>",
          "invert_powerline": true,
          "style": "diamond",
          "template": "  {{ .CurrentDate | date .Format }} ",
          "type": "time",
          "properties": {
            "time_format": "3:04pm 05s"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "overflow": "hide",
      "newline": true,
      "segments": [
        {
          "foreground": "#7c7c7c",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "foreground": "#7c7c7c",
          "style": "plain",
          "overflow": "hide",
          "template": "\u2500\u256f",
          "type": "text"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ if .Segments.Session.SSHSession }}{{ .UserName }}@{{ .HostName }} {{ end }} {{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 3
}
